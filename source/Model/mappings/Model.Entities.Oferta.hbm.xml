<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Model" namespace="Model.Entities">
  <class name="Oferta" table="Oferta">
    <id name="Id" >
      <generator class="identity" />
    </id>
    <property name="Comprador" />
    <property name="Valor" />
    <many-to-one name="Casa" class="Casa" >
      <column name="Casa_id" />
    </many-to-one>
  </class>
</hibernate-mapping>